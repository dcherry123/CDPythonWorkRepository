print('Hello, World!')
print(1 + 2)
print(7 * 6)
print()
print("The End")
print("Python's string is easy to use")
print('We can even include "quotes" in string')
print("Hello" + " world")
greeting = "Hello"
name = "Ayush"
print (greeting + name)
# if we want a space, we can add that too
print(greeting + ' ' + name)
